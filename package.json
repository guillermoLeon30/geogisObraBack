{
  "name": "geogis",
  "version": "1.0.0",
  "description": "Serverless webpack example using ecma script",
  "scripts": {
    "dev": "env-cmd -f ./config/.env nodemon --exec babel-node src/index.js",
    "seed:dev": "env-cmd -f ./config/.env babel-node ./db/seed/index.js",
    "seed:prod": "env-cmd -f ./config/.env.production babel-node ./db/seed/index.js",
    "sls:offline": "sls offline start --skipCacheInvalidation",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1",
    "serverless-http": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "nodemon": "^1.19.2",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.11.0",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.41.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
